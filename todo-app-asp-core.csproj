    <RootNamespace>todo_app_asp_core</RootNamespace>
    <UserSecretsId>1d2a4e4f-829c-4fd2-9934-c9b493b071dc</UserSecretsId>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.EntityFrameworkCore" Version="9.0.8" />
